{
    "manifest_version": 2,
    "name": "Auto Login for ls.zebras.net",
    "description": "Keeps you logged in to into SmoothWall because it times out. ls.zebras.net SmoothWall servers only!",
    "version": "1.0.2",
    "author": "Bunnbuns",
    "browser_action": {
        "default_icon": {
          "32": "icon.png"
        }
      },
    "permissions": [
        "tabs",
        "cookies",
        "webRequest",
        "contextMenus",
        "webRequestBlocking",
        "http://ls.zebras.net/*",
        "https://ls.zebras.net:442/*"
    ],
    "background": {
        "scripts": [
            "./bg.js"
        ]
    }
}
